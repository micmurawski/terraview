{
    "name": "terraform-visualizer",
    "version": "1.0.0",
    "description": "Interactive diagram generator for Terraform code",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build:backend": "tsc -p tsconfig.json",
        "build:frontend": "node build.js",
        "build": "npm run build:backend && npm run build:frontend && copyfiles -u 1 \"src/frontend/*.html\" \"src/frontend/*.css\" dist/frontend/",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "test": "jest"
    },
    "keywords": [
        "terraform",
        "visualization",
        "diagram",
        "aws"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "commander": "^11.0.0",
        "d3": "^7.8.5",
        "elkjs": "^0.8.2",
        "express": "^4.18.2",
        "hcl2-parser": "^1.0.3",
        "open": "^9.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rete": "^2.0.1",
        "rete-area-plugin": "^2.0.0",
        "rete-auto-arrange-plugin": "^2.0.0",
        "rete-connection-plugin": "^2.0.0",
        "rete-react-plugin": "^2.0.0"
    },
    "devDependencies": {
        "@types/d3": "^7.4.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.5",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "copyfiles": "^2.4.1",
        "esbuild": "^0.25.2",
        "jest": "^29.6.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "bin": {
        "terraform-visualizer": "./dist/index.js"
    }
}
